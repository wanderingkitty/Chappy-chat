{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "outDir": "./backendDist",
	  "rootDir": "./backend",
	  "strict": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["backend/**/*"],
	"exclude": ["node_modules", "src", "public"]
  }